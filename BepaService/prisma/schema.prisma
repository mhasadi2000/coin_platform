generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URI")
}

model users {
  id                    Int     @id @default(autoincrement())
  email                 String  @unique
  coin_name             String?
  difference_percentage Int?
}

model rates {
  id        Int       @id @default(autoincrement())
  coin_name String    @unique
  timestamp DateTime?
  price     Float?
}
